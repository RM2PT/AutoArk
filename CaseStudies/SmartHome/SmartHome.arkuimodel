      <flowline source="//@uilogic.9/@workflow.0/@function.2" target="//@uilogic.9/@workflow.0/@function.3" type="condition" value="exp1"/>
      <flowline source="//@uilogic.9/@workflow.0/@function.2" target="//@uilogic.9/@workflow.0/@function.4" type="condition" value="exp2"/>
      <flowline source="//@uilogic.9/@workflow.0/@function.3" target="//@uilogic.9/@workflow.0/@function.1"/>
